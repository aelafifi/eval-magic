{
  "name": "eval-magic",
  "version": "0.0.4",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build:ts": "tsc",
    "build:node": "esbuild ./src/index.ts --bundle --outfile=./build/index.js --platform=node --format=cjs",
    "build:types": "tsc --emitDeclarationOnly --declaration --outDir ./build",
    "build": "npm run build:ts && npm run build:node && npm run build:types",
    "prepublishOnly": "npm run build"
  },
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "require": "./build/index.js",
      "types": "./build/index.d.ts",
      "import": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "keywords": [],
  "author": "ahmed.s.elafifi@gmail.com",
  "license": "ISC",
  "devDependencies": {
    "@types/acorn": "^6.0.0",
    "@types/escodegen": "^0.0.10",
    "esbuild": "0.25.0",
    "prettier": "^3.3.3",
    "typescript": "^5.6.2"
  },
  "dependencies": {
    "acorn": "^8.12.1",
    "escodegen": "^2.1.0",
    "estree-walk-plus": "^0.0.2"
  }
}
